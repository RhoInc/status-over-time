{
  "name": "webcharts",
  "description": "A library for creating flexible, interactive charts",
  "version": "1.6.1",
  "keywords": [
    "charts",
    "javascript",
    "interactive",
    "data visualization"
  ],
  "homepage": "https://github.com/RhoInc/Webcharts",
  "main": "lib/webcharts.common.js",
  "jsnext:main": "src/index.js",
  "dependencies": {
    "d3": ">= 3.0.0"
  },
  "scripts": {
    "es6": "rollup -g d3 src/index.js > lib/webcharts.es6.js",
    "common": "rollup src/index.js -g d3 | babel > lib/webcharts.common.js &&  sed -i \"s/require('d3')/var d3 = require('d3')/\" lib/webcharts.common.js",
    "amd": "rollup src/index.js -f amd -g d3 | babel > lib/webcharts.amd.js",
    "es5": "rollup src/index.js -g d3 | babel --modules ignore | cat lib/open.js - lib/close.js > build/webcharts.js",
    "uglify": "uglifyjs build/webcharts.js > build/webcharts.min.js",
    "build": "npm run es5 && npm run uglify",
    "all": "npm run common && npm run es6 && npm run amd && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhoinc/webcharts.git"
  },
